<% include ../partials/header %>

<% let confirm = "";
    if (payment.status == "paid") {
        confirm = "succesvol"
    } else if (payment.status == "open" || payment.status == "expired" || payment.status == "failed" || payment.status == "canceled" ) {
        confirm = "niet gelukt"
    } %>

<div class="row justify-content-center">
    <h1 class="col col-md-8">
        Bestelling <%= payment.description%> <%=confirm%>
    </h1>
</div>


<% include ../partials/footer %>